{
  "name": "docz-plugin-vue",
  "version": "0.1.0",
  "description": "Plugin that integrates support for Vue components into Docz",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "libundler watch --ts -e all",
    "build": "libundler build --ts -e all --c",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project .",
    "tslint": "tslint --project ."
  },
  "keywords": [
    "docz",
    "plugin",
    "vue"
  ],
  "dependencies": {
    "docz-core": "^0.3.2",
    "vuera": "^0.2.2"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "typescript": "^2.9.2",
    "vue": "^2.5.16",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.5.16"
  }
}
