{
  "name": "docz",
  "version": "2.4.0",
  "description": "It's has never been so easy to documents your things!",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/doczjs/docz.git",
    "directory": "packages/docz"
  },
  "bin": {
    "docz": "./bin/index.js"
  },
  "publishConfig": {
    "main": "./dist/index.js"
  },
  "main": "src/index.ts",
  "access": "public",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./astro": {
      "default": "./astro.mjs"
    }
  },
  "files": [
    "bin/",
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --dts",
    "build:watch": "tsup --dts --watch"
  },
  "dependencies": {
    "@docz/core": "workspace:*",
    "ci-info": "^3.4.0",
    "nanostores": "^0.7.0",
    "path-to-regexp": "^6.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "supports-esm": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6"
  }
}
