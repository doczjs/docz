{
  "name": "docz",
  "description": "Blazing fast and zero config React components playground",
  "version": "0.0.1",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "bin": {
    "docz": "./bin/index.js"
  },
  "files": [
    "bin/",
    "dist/",
