    "fix:tslint": "tslint --fix --project .",
    "tslint": "tslint --project ."
  },
  "dependencies": {
    "autoprefixer": "^9.0.1",
    "css-loader": "^1.0.0",
    "deepmerge": "^2.1.1",
    "docz-core": "^0.8.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "loader-utils": "^1.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss": "^7.0.1",
    "postcss-flexbugs-fixes": "^4.0.0",
    "postcss-loader": "^2.1.6",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "webpack": "^4.16.3",
    "webpack-chain": "^4.8.0"
