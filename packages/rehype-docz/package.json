    "fix:tslint": "tslint --fix --project .",
    "tslint": "tslint --project ."
  },
  "dependencies": {
    "@babel/parser": "^7.0.0",
    "@babel/traverse": "^7.0.0",
    "hast-util-to-string": "^1.0.1",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "prettier": "^1.14.2",
    "signale": "^1.2.1",
    "strip-indent": "^2.0.0",
    "unist-util-is": "^2.1.2"
  },
  "devDependencies": {
    "@types/lodash.flatten": "^4.4.4",
    "@types/lodash.get": "^4.4.4"
  }
}
