{
  "name": "rehype-docz",
  "version": "0.10.3",
  "description": "Rehype plugin used by docz",
  "license": "MIT",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "typings": "dist/index.d.ts",
  "source": "src/index.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "dev": "libundler watch -e all --ts",
    "build": "libundler build -e all --ts --c",
    "fix": "run-s fix:*",
