{
  "name": "playgrodd",
  "description": "Blazing fast and zero config React components playground",
  "version": "0.0.1",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.m.js",
  "source": "src/index.ts",
  "bin": {
    "playgrodd": "./bin/index.js"
  },
  "scripts": {
    "dev": "libundler watch --ts",
    "build": "libundler build --ts --compress --sourcemap",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.{ts,tsx}\" --write",
    "fix:tslint": "tslint --fix --project ."
  },
  "dependencies": {
    "playgrodd-core": "^0.0.1",
    "playgrodd-bundler-webpack": "^0.0.1",
    "playgrodd-theme-default": "^0.0.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-router-dom": "^4.2.2",
    "ulid": "^2.3.0",
    "unstated": "^1.1.0",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "react": "^16.3.1",
    "react-dom": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^9.6.4",
    "@types/react": "^16.3.8",
    "@types/react-dom": "^16.0.5",
    "@types/react-router-dom": "^4.2.6",
    "@types/yargs": "^11.0.0"
  }
}
